  <% tt = TypeText.instance %>
  <tr id="<%= dom_id row %>" <%= 'class=deleted' unless row.status && row.status == 1 %>>
      <td><%= row.id %></td>
      <td><%= row.company_id %></td>
      <td><%= row.nickname %></td>
      <td><%= tt.get_texts('roles', row.roles) %></td>
      <td><%= time_tag row.employ_date if row.employ_date %></td>
      <td><%= row.ssn %></td>
      <td><%= row.birthday %></td>
      <td><%= row.status %></td>
    <td class="actions">
      <%= link_to_edit row %> |
      <%= link_to 'Photos', admin_employee_info_photos_path(row), {:target => 'edit-win'} %> |
      <%= link_to_hide row, 'data-confirm' => 'Are you sure to hide/show this Employee?' %>
    </td>
  </tr>
