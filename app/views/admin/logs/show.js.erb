var edit_div = "edit_log_div";
if ($('#'+edit_div).length == 0){
  $('#page_main').append("<div id='"+ edit_div +"'></div>");
  $('#'+edit_div).dialog({ autoOpen:false, width:700, dialogClass:'shadow', modal:true});
}

$('#'+edit_div)
.html("<%= j render(:partial => "detail", :locals => {:log => @object}) %>")
	.dialog('option', 'title', 'View Log <%= @object.id%>#')
	.dialog('open');

